package com.ustglobal.maps;

import java.util.HashMap;

public class TestA {

	public static void main(String[] args) {
		
		HashMap hm = new HashMap<>();
		hm.put("Nisha", 8665);
		hm.put("Vidya", 757446);
		hm.put("Anshika", 574633);
		hm.put("Diksha", 74625252);
		hm.put(null, 87746);
		hm.put(null, 865446);
		
		System.out.println("Data " +hm);
		hm.put("Vidya", 6855546);
		System.out.println("After Modification " +hm);
		hm.put("Dimple", 8665 );
		System.out.println("After dimple " +hm);
		System.out.println("After null" +hm);
		System.out.println("=========================");
		System.out.println("After adding duplicate null");
		System.out.println(hm);
		System.out.println("==========================");
		Object phoneno = hm.get("Sheela");
		System.out.println("Value " +phoneno);
	}
}
