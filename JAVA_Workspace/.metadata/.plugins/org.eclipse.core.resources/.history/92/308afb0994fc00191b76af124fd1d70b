package com.ustglobal.collectionframework.list;

import java.util.ArrayList;

public class TestA {

	public static void main(String[] args) {

		ArrayList<Comparable> al = new ArrayList<Comparable>();
		al.add(12);
		al.add(2.5);
		al.add("Vardhan");
		al.add(true);
		al.add(null);
		al.add('A');
		
		Object o = al.get(2);
		System.out.println(o);
		
		for(int i = 0 ; i < 6 ; i++) {
			Object obj = al.get(i);
			System.out.println(obj);
		}
	}

}
