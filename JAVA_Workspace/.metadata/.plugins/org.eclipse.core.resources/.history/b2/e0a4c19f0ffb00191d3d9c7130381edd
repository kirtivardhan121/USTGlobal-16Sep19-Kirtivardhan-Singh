package com.ustglobal.stringclass;

public class TestA {

	public static void main(String[] args) {
		
		String s1 = "Aishu";
		String s2 = "Nishu";
		
		String s3 = new String("Nishi");
		String s4 = new String("Nishi");
		String s5 = "Aishu";
		
		System.out.println(s1==s2);
		System.out.println(s3==s4);
		System.out.println(s1==s5);
	}
}
