package com.ustglobal.maps;

import java.util.Hashtable;

public class TestH {

	public static void main(String[] args) {
		
		Hashtable<Integer, String> ht = new Hashtable<>();
		ht.put(1, "Ajay");
		ht.put(3, "Vijay");
		ht.put(2, "Sonu");
		ht.put(4, "Monu");
		ht.put(1, "Sriram");
//		ht.put(null, "John"); //we can not add null 
		ht.put(8, null);
		System.out.println("Data " +ht);
	}
}
