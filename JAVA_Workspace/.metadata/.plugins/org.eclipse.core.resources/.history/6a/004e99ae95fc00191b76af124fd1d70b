package com.ustglobal.collectionframework.list;

import java.util.ArrayList;

public class TestC {

	public static void main(String[] args) {
		
		 ArrayList al = new ArrayList<E>();
		 
		 al.add(23);
		 al.add(98.7);
		 al.add(true);
		 al.add("Good Evening");
		 
		 for (int i = 0 ; i < al.size() ; i++) {
			 Object o = al.get(i);
			 System.out.println(o);
		 }
	}
}
